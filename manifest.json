{
	"name": "SABconnect++",
	"version": "0.4.11",
	"description": "SABnzbd extension for Google Chrome.",
	"background_page": "background.html",
	"options_page": "settings.html",
	"browser_action": {
		"default_icon": "images/sab2_16.png",
		"default_title": "SABnzbd",
		"popup": {
			"height": 200,
			"path": "popup.html"
		}
	},
	"content_scripts": [
		{
			"matches": [
				"http://*.newzbin.com/*",
				"https://*.newzbin.com/*",
				"http://*.newzxxx.com/*",
				"https://*.newzxxx.com/*"
			],
			"js": ["jquery-1.4.2.min.js", "common.js", "content_scripts/newzbin.js"]
		},
		{
			"matches": [
				"http://*.nzbmatrix.com/",
				"http://*.nzbmatrix.com/*",
				"https://*.nzbmatrix.com/",
				"https://*.nzbmatrix.com/*",
				"http://*.nzbxxx.com/",
				"http://*.nzbxxx.com/*",
				"https://*.nzbxxx.com/",
				"https://*.nzbxxx.com/*"
			],
			"js": ["jquery-1.4.2.min.js", "common.js", "content_scripts/nzbmatrix.js"],
			"css": ["css/nzbmatrix.css"]
		},
		{
			"matches": [
				"http://*.nzbclub.com/",
				"http://*.nzbclub.com/*",
				"https://*.nzbclub.com/",
				"https://*.nzbclub.com/*"
			],
			"js": ["jquery-1.4.2.min.js", "common.js", "content_scripts/nzbclub.js"],
			"all_frames": true
		},
		{
			"matches": [
				"http://*.bintube.com/",
				"http://*.bintube.com/*",
				"https://*.bintube.com/",
				"https://*.bintube.com/*"
			],
			"js": ["jquery-1.4.2.min.js", "common.js", "content_scripts/bintube.js"],
			"all_frames": true
		},
		{
			"matches": [
				"http://*.nzbs.org/",
				"http://*.nzbs.org/*",
				"https://*.nzbs.org/",
				"https://*.nzbs.org/*"
			],
			"js": ["jquery-1.4.2.min.js", "common.js", "content_scripts/nzbsorg.js"],
			"all_frames": true
		},
		{
			"matches": [
				"http://*.binsearch.info/",
				"http://*.binsearch.info/*",
				"https://*.binsearch.info/",
				"https://*.binsearch.info/*",
				"http://binsearch.info/",
				"http://binsearch.info/*",
				"https://binsearch.info/",
				"https://binsearch.info/*",
				"http://*.binsearch.net/",
				"http://*.binsearch.net/*",
				"https://*.binsearch.net/",
				"https://*.binsearch.net/*",
				"http://binsearch.net/",
				"http://binsearch.net/*",
				"https://binsearch.net/",
 				"https://binsearch.net/*",
 				"http://*.binsearch.co.uk/",
 				"http://*.binsearch.co.uk/*",
 				"https://*.binsearch.co.uk/",
 				"https://*.binsearch.co.uk/*",
 				"http://binsearch.co.uk/",
 				"http://binsearch.co.uk/*",
 				"https://binsearch.co.uk/",
 				"https://binsearch.co.uk/*",
 				"http://*.binsear.ch/",
 				"http://*.binsear.ch/*",
 				"https://*.binsear.ch/",
 				"https://*.binsear.ch/*",
 				"http://binsear.ch/",
 				"http://binsear.ch/*",
 				"https://binsear.ch/",
 				"https://binsear.ch/*"
			],
			"js": ["jquery-1.4.2.min.js", "common.js", "content_scripts/binsearch.js"],
			"all_frames": true
		},
		{
			"matches": [
				"http://*.newzleech.com/",
				"http://*.newzleech.com/*",
				"https://*.newzleech.com/",
				"https://*.newzleech.com/*",
				"http://newzleech.com/",
				"http://newzleech.com/*",
				"https://newzleech.com/",
				"https://newzleech.com/*"
			],
			"js": ["jquery-1.4.2.min.js", "common.js", "content_scripts/newzleech.js"],
			"all_frames": true
		},
		{
			"matches": [
				"http://*.nzbindex.com/",
				"http://*.nzbindex.com/*",
				"https://*.nzbindex.com/",
				"https://*.nzbindex.com/*",
				"http://nzbindex.com/",
				"http://nzbindex.com/*",
				"https://nzbindex.com/",
				"https://nzbindex.com/*",
				"http://*.nzbindex.nl/",
				"http://*.nzbindex.nl/*",
				"https://*.nzbindex.nl/",
				"https://*.nzbindex.nl/*",
				"http://nzbindex.nl/",
				"http://nzbindex.nl/*",
				"https://nzbindex.nl/",
				"https://nzbindex.nl/*"
			],
			"js": ["jquery-1.4.2.min.js", "common.js", "content_scripts/nzbindex.js"],
			"all_frames": true
		},
		{
			"matches": [
				"http://*.nzbsrus.com/",
				"http://*.nzbsrus.com/*",
				"https://*.nzbsrus.com/",
				"https://*.nzbsrus.com/*",
				"http://nzbsrus.com/",
				"http://nzbsrus.com/*",
				"https://nzbsrus.com/",
				"https://nzbsrus.com/*"
			],
			"js": ["jquery-1.4.2.min.js", "common.js", "webtoolkit.base64.js", "content_scripts/nzbsrus.js"],
			"all_frames": true
		},
		{
			"matches": [
				"http://*.nzb.su/",
				"http://*.nzb.su/*",
				"http://nzb.su/",
				"http://nzb.su/*",
				"https://*.nzb.su/",
				"https://*.nzb.su/*",
				"https://nzb.su/",
				"https://nzb.su/*"
			],
			"js": ["jquery-1.4.2.min.js", "common.js", "webtoolkit.base64.js", "content_scripts/nzb.su.js"],
			"all_frames": true
		}
	],
	"icons": { "16": "images/sab2_16.png",
			   "32": "images/sab2_32.png",
			  "128": "images/sab2_128.png" },
	"permissions": [
		"http://*/*",
		"https://*/*",
		"tabs",
		"notifications"
	]
}
